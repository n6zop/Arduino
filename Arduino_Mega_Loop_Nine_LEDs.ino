  digitalWrite(26, LOW);
  digitalWrite(28, HIGH);
  delay(25);
  digitalWrite(28, LOW);
  digitalWrite(30, HIGH);
  delay(25);
  digitalWrite(30, LOW);
  digitalWrite(32, HIGH);
  delay(25);
  digitalWrite(32, LOW);
  digitalWrite(34, HIGH);
  delay(25);
  digitalWrite(34, LOW);
  digitalWrite(36, HIGH);
  delay(25);
  digitalWrite(36, LOW);
  digitalWrite(38, HIGH);
  delay(25);
  digitalWrite(38, LOW);
